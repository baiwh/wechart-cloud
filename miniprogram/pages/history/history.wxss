page {
  background: #f6f6f6;
}
.history-echarts {
  position: absolute; 
  height: 1200rpx;
  top: -10%;
  left: 0;
  right: 0;
}
picker{
  border-top: 1rpx solid #dcdcdc;
  border-bottom: 1rpx solid #dcdcdc;
  width: 300rpx;
  color: #adadad;
  text-align: center;
  transform: translateX(-50%);
  top: 50rpx;
  left: 50%;
  position: relative;
  line-height: 70rpx;
  z-index: 100;
}
.sign-in-data{
  background: #f6f6f6;
}
.sign-in-btn{
  height: 50rpx;
  width: 50rpx;
  border-radius: 50%;
}
